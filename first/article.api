type (
	ArticleReq {
		Title  string `json:"title"`
		Link   string `json:"link"`
		Poster string `json:"poster"`
		Time   string `json:"time"`
		Votes  int    `json:"votes"`
	}

	ArticleResp {
		Code int    `json:"code"`
		Msg  string `json:"msg"`
	}
)

@server(
	prefix: first/v1
	group: friend
)

service practice {
	@server(
		handler: createArticle
	)
	
	post /first/article(ArticleReq) returns(ArticleResp)
}